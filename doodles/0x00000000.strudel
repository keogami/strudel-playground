setCpm(25)

$: s("sbd sbd rim [hh:0 hh:1] [bd hh:0] bd rim [hh:0 hh:1]")
  .swingBy(1/4, 8)
  .lpf(saw.range(800, 1100))
  .gain(sine.range(.25, .55))
  ._punchcard()
$: n("0, 2, <4 [4, 6] [5, 7]>")
  .scale("<<c:minor f:minor> e:major <d:minor g2:major>>")
  .segment(8)
  .gain(sine.range(.25, 1))
  .lpf(200)
  .s("sawtooth")
  ._pianoroll()